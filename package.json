{
  "name": "open-s3",
  "version": "1.0.0",
  "description": "Terminal UI for S3 bucket exploration",
  "main": "src/index.ts",
  "type": "module",
  "bin": {
    "open-s3": "./src/index.ts"
  },
  "scripts": {
    "start": "bun run src/index.ts",
    "dev": "bun run --watch src/index.ts",
    "build": "bun build src/index.ts --outdir dist --target bun",
    "test": "bun test"
  },
  "keywords": [
    "s3",
    "tui",
    "terminal",
    "explorer"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.932.0",
    "@opentui/core": "^0.1.44"
  }
}
